  "repository": {
    "type": "git",
    "url": "https://github.com/dignifiedquire/borc.git"
  },
  "homepage": "https://github.com/dignifiedquire/borc.git",
  "browser": {
    "fs": false
  },
  "scripts": {
    "lint-asm": "asmjs lib/*.asm.js",
    "test": "aegir-test",
    "test:browser": "aegir-test --env browser",
    "test:node": "aegir-test --env node",
    "lint": "aegir-lint",
