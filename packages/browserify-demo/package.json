{
  "name": "browserify-demo",
  "version": "7.0.5",
  "description": "",
  "private": "true",
  "scripts": {
    "prebuild": "rm -rf dist; mkdir dist",
    "build": "browserify --standalone cbor ../cbor/lib/cbor.js -t [ babelify  ] | terser --compress -o dist/bundle-bf.js; browserify src/index.js -t [ babelify  ] -o dist/index-bf.js; cp src/*.css src/*.html dist",
    "deploy": "cp dist/* ../../docs/example",
    "predeploy": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/hildjj/node-cbor.git",
    "directory": "packages/browserify-demo"
  },
  "keywords": [],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "node-inspect-extracted": "*",
    "terser": "^5.7.1"
  }
}
